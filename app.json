{
  "expo": {
    "name": "Wealth Walk",
    "slug": "wealthwalk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/wealthwalklogo.png",
    "scheme": "wealthwalk",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wealthwalklogo.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.wealth.walk"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/wealthwalklogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/wealthwalklogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-audio",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "11834245-fd6c-4f9b-b94f-917ade2267d1"
      },
      "apiBaseUrl": "http://192.168.1.37:3000/api"
    }
  }
}
